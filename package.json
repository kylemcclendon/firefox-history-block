    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.1"
  },
